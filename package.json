{
  "name": "mongo-message",
  "version": "0.0.28",
  "description": "Using mongo as message queue",
  "jsnext:main": "dist/index",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/jest --coverage src/*.test.ts",
    "prepublish": "npm run build"
  },
  "authors": [
    "Tran Hoai Nam <ntran.picom@gmail.com> (https://github.com/namtran3005)"
  ],
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.1",
    "mongoose": "^5.1.1",
    "uuid": "^3.2.1",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/jest": "^22.2.3",
    "@types/mongoose": "^5.0.14",
    "@types/node": "^10.1.1",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.9",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/src/.*\\.(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": "git@github.com:namtran3005/mongo-message.git",
  "author": "namtran3005 <ntran.picom@gmail.com>"
