{
  "name": "mongo-message",
  "version": "0.0.22",
  "description": "Using mongo as message queue",
  "jsnext:main": "dist/index",
  "main": "dist/index",
  "types": "dist/index",
  "scripts": {
    "build": "babel src --out-dir lib && ./node_modules/.bin/flow-copy-source src lib",
    "buildts": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/jest --coverage src/*.test.ts",
    "prepublish": "npm run buildts"
  },
  "authors": [
    "Tran Hoai Nam <ntran.picom@gmail.com> (https://github.com/namtran3005)"
  ],
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.1",
    "mongoose": "^4.12.1",
    "uuid": "^3.1.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.18",
    "@types/jest": "^21.1.6",
    "@types/mongoose": "^4.7.27",
    "@types/node": "^8.0.53",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.7",
    "jest": "^21.2.1",
    "ts-jest": "^21.2.3",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/src/.*\\.(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": "git@github.com:namtran3005/mongo-message.git",
  "author": "namtran3005 <ntran.picom@gmail.com>"
}
